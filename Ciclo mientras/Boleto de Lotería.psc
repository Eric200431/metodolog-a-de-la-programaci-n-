Proceso Boleto_de_loteria
	Definir boleto Como Entero;
	Escribir "Algoritmo: Boleto de loteria";
	Escribir "Autor:Eric Estrada";
	Escribir "Ingresa el boleto que";
	Leer boleto;
	Mientras boleto >= 1 y boleto >= 100 Hacer
	Escribir "Ingresa el boleto";
